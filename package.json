{
  "name": "media-wifi-pc",
  "version": "1.0.0",
  "description": "Control Your media playing in laptop over web ui and local wifi",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "pkg": "pkg ."
  },
  "keywords": [
    "theinfinox"
  ],
  "author": "theinfinox",
  "license": "ISC",
  "dependencies": {
    "brightness": "^3.0.0",
    "express": "^4.21.2",
    "loudness": "^0.4.2",
    "mongodb": "^6.13.1",
    "rcedit": "^4.0.1",
    "robotjs": "^0.6.0",
    "socket.io": "^4.8.1"
  },
  "bin": "./app.js",
  "pkg": {
    "assets": [
      "public/**",
      "node_modules/loudness/impl/windows/*.exe",
      "node_modules/robotjs/build/Release/*.node"
    ],
    "outputPath": "dist",
    "targets": [
      "node18-win-x64"
    ],
     "icon": "public/icon/icon.ico"
  }
}
